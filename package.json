{
  "name": "fxjs",
  "version": "1.0.0",
  "description": "Reactive programming library for javascript.",
  "main": "index.js",
  "jest": {
    "testMatch": [
      "**/__tests__/**/*.?(m)js?(x)",
      "**/?(*.)(spec|test).?(m)js?(x)"
    ],
    "moduleFileExtensions": [
      "js",
      "json",
      "jsx",
      "node",
      "mjs"
    ],
    "transform": {
      "^.+.m?js$": "babel-jest"
    }
  },
  "babel": {
    "env": {
      "test": {
        "presets": [
          [
            "es2015",
            {
              "modules": false
            }
          ]
        ],
        "plugins": [
          [
            "transform-es2015-modules-commonjs",
            {
              "spec": true
            }
          ]
        ]
      }
    }
  },
  "scripts": {
    "test": "jest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/amalshehu/FxJs.git"
  },
  "author": "Amal Ahehu",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/amalshehu/FxJs/issues"
  },
  "homepage": "https://github.com/amalshehu/FxJs#readme",
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-jest": "^23.0.1",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.26.2",
    "babel-preset-es2015": "^6.24.1",
    "eventemitter3": "^3.1.0",
    "jest": "^23.1.0",
    "node-uuid": "^1.4.8"
  }
}
